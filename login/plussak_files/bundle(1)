module.exports.mustBe = function (value, arr) {
    return (arr.indexOf(value) !== -1);
};


// Encode secure URL

module.exports.secureEncode = function (u) {
    var str = u.replace(/\//g, '_');
    return encodeURIComponent(str);
};


