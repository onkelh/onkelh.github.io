(function (process,global,Buffer,__argument0,__argument1,__argument2,__argument3,__filename,__dirname){
'use strict';

var Promise = require('bluebird'),
    obscura = require('obscura-node-client'),
    log     = require('../../../../lib/logger.js'),
    util    = require('util');

/**
 * Usage:
 * {{obscuraImagePath attribute="value"}}
 *
 * Attributes:
 * - backend   = string, e.g. 'escenic'
 * - imagePath = string, relative path to image, e.g. '/archive/05160/BA44282_5160376a.jpg'
 * - width     = int
 * - height    = int
 * - maxWidth  = int, max image width, e.g. 980
 */
function obscuraPath(obscuraUrl, options) {
    var width       = Math.round(Math.min(options.maxWidth, options.width)),
        height      = Math.round(width * options.ratio);

    obscura.configure(obscuraUrl);

    return obscura.buildDynamic(options.backend, options.imagePath, '0000', [
        {
            name : 'resize',
            width : width,
            height : height,
            strategy : 'l',
            upsizable : false
        }
    ]);
}

/**
 * Get image ratio. Fallback value is 16/9 ratio.
 **/
function getRatio(height, width) {
    return (typeof height === 'number' && typeof width === 'number') ? (height / width) : 0.5625;
}

 /**
 * Get image sizes (media queries) as a string
 **/
function getImageSizes(minWidth, maxWidth, width) {
    var sizes = [];

    if (maxWidth && width) {
        sizes.push(util.format('(max-width: %spx) %spx', maxWidth, width));
    }
    if (minWidth && width) {
        sizes.push(util.format('(min-width: %spx) %spx', minWidth, width));
    }

    //Return media queries, delimited by ' and ' if more than one
    return (sizes.length > 1 ? sizes.join(' and ') : sizes.toString());
}

/**
 * Get URL for image, use placeholder image if ?placeholders=img is set
 **/
function getDefaultImagePath(obscuraImagePath, options, data, width, ratio) {
    if(options.placeholder) {
        return util.format('http://placehold.it/%sx%s', width, (width * ratio));
    }else {
        return obscuraImagePath({
            backend: data.fields.obscuraBackend || 'escenic',
            imagePath: data.fields.relativeUrl || '',
            maxWidth: options.maxWidth || 1080,
            width: width,
            ratio: ratio
        });
    }
}

/**
*  Transform image data for the template
**/
function transformEach(options, obscuraImagePath, data) {
    if (!data.fields) {
        return log.warn('Missing data.fields for image object');
    }

    var ratio = options.ratio ? options.ratio : getRatio(data.fields.height, data.fields.width),
        sizes = [],
        srcset= [];

    //Set image padding based on ratio
    if(options.useRatio != false) {
        //If ratio is undefined we set it to true
        options.useRatio = true;
        //Set ratio as percentage
        data.ratioString = util.format('style="padding-top:%s%%";', (ratio*100));
    }

    //Fix for article-types that does not set caption check like they should
    data.showCaption = options.noCaption ? false : (data.fields.photographer || data.caption);

    //Set image size variants that we include in the <img> tag
    if(options.variants) {
        srcset = options.variants.map(function (variant, index) {
            var width         = variant.width || 300;

            //Read media queries from template frontmatter
            if(variant.minWidth || variant.maxWidth) {
                sizes.push(getImageSizes(variant.minWidth, variant.maxWidth, width));
            }

            //When rendering on client side, the url is already set
            return  variant.src || util.format('%s %sw',
                        getDefaultImagePath(obscuraImagePath, options, data, width, ratio),
                        width);
        });

        //Create delimited strings for template
        data.srcset = srcset.join(', ');
        data.sizes  = sizes.join(', ');
    }

    //Set default image url
    data.src = options.src || getDefaultImagePath(obscuraImagePath, options, data, (options.width || 300), ratio);
}

module.exports = function imageTransform(templateData) {
    var options          = templateData.options,
        data             = templateData.data,
        obscuraImagePath = obscuraPath.bind(null, templateData.root.env.gaia['obscura.url']),
        params           = templateData.root.env.params;

    var transform           = transformEach.bind(null, options, obscuraImagePath);
        options.placeholder = (params && params.placeholders && params.placeholders.indexOf('img') !== -1);

    if (Object.prototype.toString.call(data) === '[object Array]') {
        data.forEach(transform);
    } else {
        transform(data);
    }

    return Promise.resolve(templateData);
}

